        <div class="tab-pane-left-col">
          <%= render 'admin/contracts/shared/astra_team/contract_info' %>
        </div>

        <div class="tab-pane-right-col">
          <%= render 'admin/contracts/shared/astra_team/table', contracts: @contracts %>

        </div>
