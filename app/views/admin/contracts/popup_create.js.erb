<% if @use_case.failure? %>
  alert('<%= @use_case.error.first %>');
<% else %>
  $('#addNewUser').modal('hide');
  $('.user-table').prepend('<%= j render "admin/contracts/shared/astra_team/table_row", contract: @use_case.contract %>');

  $('#modals-container #changeRole').html("<%= j render 'admin/users/role_popup', user: @use_case.contract.user, role: Role.new %>");
  setTimeout(function() {
    $('#changeRole').modal('show');
  }, 1000)

<% end %>
